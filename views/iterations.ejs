
<div class="page-header">
  <h1><a id="hproject" href="/projects/<%= project.id %>"><%= project.name %></a> - <%= title %></h1>

  <ul class="nav nav-pills">
      <li><a href="#">Todos</a></li>
      <li class="active"><a href="#current">Actual</a></li>
      <li><a href="#current_backlog">Actual y Backlog</a></li>
      <li><a href="#backlog">Backlog</a></li>
      <li><a href="#done/1">Terminados</a></li>
  </ul>
</div>
<div class="row">
  <div class="span8">
    <ul id="sprints"></ul>
  </div>
</div>

<script id="tmplSprint" type="text/x-jquery-tmpl">
  <li>
    <p><b>Sprint #${number}</b> - 
    	<a href="/projects/<%= project.id %>/iterations/burnDown" target="_blank">burn down</a>
    	</p>
    <p>Inicio: ${start}</p>
    <p>Fin: ${end}</p>
    <p>FF: %${strength}</p>
    <p>VE: ${storyAmm}</p>
    <p>TE: ${tasksAmm}</p>
    <a class="stories">Historias (${stories.length})</a>&nbsp;&nbsp;
    <a href="/projects/<%= project.id %>/iterations/${number}/print" target="_blank">imprimir</a>
    <ul class="storyList">
      {{each stories}}
        <li>
          [${estimate} pts] ${name}
        </li>
      {{/each}}
    </ul>
    <hr/>
  </li>
</script>

<script type="text/javascript" src="/js/views/iteration.js" ></script>