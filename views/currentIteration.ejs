<b>Sprint #<%= iteration.number %></b>
<br/>
<p>Inicio: <%= iteration.start %></p>
<p>Fin: <%= iteration.end %></p>
<p>FF: %<%= iteration.strength %></p>
<p>VE: <%= iteration.storyAmm %></p>
<p>TE: <%= iteration.tasksAmm %></p>
<hr/>
<% if (iteration.stories.length) { %>
  <ul>
    <% iteration.stories.forEach(function(s){ %>
      <li>

        <%= s.name %>
        <% if (s.tasks.length) { %>
          <ul>
            <% s.tasks.forEach(function(t){ %>
              <li>
                <%= t.description %>
                <br/>
                <%= t.estimate %>
              </li>
            <% }) %>
          </ul>
        <% } %>
                
      </li>
    <% }) %>
  </ul>
<% } %>
